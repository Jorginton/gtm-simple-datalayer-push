//Created by Ventastic Solutions - Jorg van de Ven
//Contact me on LinkedIn: linkedin.com/in/jorgvandeven/

const createQueue = require('createQueue');
const dataLayerPush = createQueue('dataLayer');
const copyFromDataLayer = require('copyFromDataLayer');
const log = require('logToConsole');

let eventName = data.eventName;
let eventValue = data.eventValue;

let customValueName;
const cVN = data.customValueName;

if (cVN === 'custom') {
  customValueName = data.delimiter;
} else {
  customValueName = 'value';
}

var key = customValueName;
var obj = {};
obj[key] = eventValue;

dataLayerPush(obj, {'event': eventName});

log('Ventastic Solutions - Simple dataLayer Push:' + '\n' + 'event_name: ' + eventName + '\n' + customValueName + ': ' + eventValue);

data.gtmOnSuccess();


